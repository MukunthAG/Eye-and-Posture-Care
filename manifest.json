{
    "name" : "Eye & Posture Care",
    "version" : "0.0.1",
    "description" : 
    "A simple app which periodically reminds you to relax your Eyes and adjust your Posture.",
    "icons" : {
        "16" : "icons/logo-16_x_16.png",
        "32" : "icons/logo-32_x_32.png",
        "48" : "icons/logo-48_x_48.png",
        "64" : "icons/logo-64_x_64.png",
        "128" : "icons/logo-128_x_128.png"
    },
    "permissions" : ["storage", "tabs", "notifications"],
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "background" : {
        "scripts" : ["notifier.js"]
    },
    "manifest_version" : 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}


